pipeline {
  agent any
  stages {
    stage('Plan') {
      steps {
        echo 'Plan required for application'
      }
    }

    stage('Code') {
      steps {
        echo 'Code to develop'
      }
    }

    stage('Build') {
      steps {
        echo 'Code should be build'
      }
    }

    stage('Test') {
      steps {
        echo 'Test the code'
      }
    }

    stage('Release') {
      steps {
        echo 'Release the application'
      }
    }

    stage('Deploy') {
      steps {
        echo 'deploying the application'
      }
    }

    stage('Operate') {
      steps {
        echo 'Operate the application'
      }
    }

  }
}